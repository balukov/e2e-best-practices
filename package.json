{
  "name": "wdio-boilerplate",
  "version": "0.0.3",
  "description": "The boilerplate project with advice best practices for e2e tests",
  "author": "Ivan Balukov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/balukov/e2e-best-practices/issues"
  },
  "homepage": "https://github.com/balukov/e2e-best-practices#readme",
  "scripts": {
    "test": "wdio ./wdio.conf.js",
    "tslint": "tslint --project tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint",
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balukov/e2e-best-practices.git"
  },
  "keywords": [
    "best practice",
    "webdriverio",
    "mocha",
    "chai",
    "allure",
    "page",
    "object"
  ],
  "dependencies": {
    "@types/chai": "4.2.3",
    "@types/mocha": "5.2.7",
    "@types/node": "12.11.1",
    "@wdio/allure-reporter": "5.14.5",
    "@wdio/cli": "5.15.1",
    "@wdio/local-runner": "5.15.1",
    "@wdio/mocha-framework": "5.15.1",
    "@wdio/selenium-standalone-service": "5.15.0",
    "@wdio/spec-reporter": "5.15.1",
    "@wdio/sync": "5.15.1",
    "chai": "4.2.0",
    "mocha": "6.2.3",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.6.4",
    "webdriverio": "5.15.1"
  },
  "devDependencies": {
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "tslint": "5.20.0",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0"
  }
}
